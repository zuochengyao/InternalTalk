<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.megvii.dao.IKeyDao">
	
	<resultMap id="T_Key_ResultMap" type="com.megvii.model.Key">
		<result column="k_key" property="key" jdbcType="VARCHAR"/>
		<result column="k_secret" property="secret" jdbcType="VARCHAR"/>
	</resultMap>
	
	<select id="getKeyByUserId" parameterType="java.lang.Integer" resultMap="T_Key_ResultMap">
		SELECT k_key, k_secret FROM t_key WHERE k_user_id = #{userId}
	</select>
	
</mapper>